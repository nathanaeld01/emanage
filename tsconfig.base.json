{
	"compilerOptions": {
		"composite": true,
		"customConditions": ["development"],
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"importHelpers": true,
		"isolatedModules": true,
		"lib": ["es2022"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"paths": {
			"@emanage/ui/*": ["./libs/ui/src/ui/*"],
			"@emanage/ui/utils": ["./libs/ui/src/utils.ts"],
			"@emanage/ui/styles/*": ["./libs/ui/src/styles/*"]
		},
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022"
	}
}
